{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "pre-push": "echo \"Building website!\" && npm run build",
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build && rm -rf css img js && mv dist/* ./",
    "lint": "vue-cli-service lint"
  },
  "pre-push": ["pre-push"],
  "dependencies": {
    "anvaad-js": "^1.0.3",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.9",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.9",
    "@vue/cli-service": "^3.0.0-beta.9",
    "pre-push": "^0.1.1",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": ["@vue/app"]
  },
  "eslintConfig": {
    "root": true,
    "extends": ["plugin:vue/essential", "eslint:recommended"]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"]
}
